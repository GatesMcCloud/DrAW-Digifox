<Defs>

  <PawnKindDef Name="DigifoxPawnBase" Abstract="True">
      <race>Human</race>
      <defaultFactionType>DigifoxEnclave</defaultFactionType>
      <chemicalAddictionChance>0.02</chemicalAddictionChance>
      <backstoryCryptosleepCommonality>0.05</backstoryCryptosleepCommonality>
      <invNutrition>2.55</invNutrition>
      <combatPower>40</combatPower>
      <initialWillRange>0~1</initialWillRange>
		  <initialResistanceRange>10~17</initialResistanceRange>
        <apparelTags>
        <li>IndustrialBasic</li>
        <li>IndustrialAdvanced</li>
        </apparelTags>
      <useFactionXenotypes>false</useFactionXenotypes>
      <xenotypeSet>
	     <xenotypeChances>
	      <DigiFox>999</DigiFox>
	     </xenotypeChances>
	      <warnIfTotalAbove1>false</warnIfTotalAbove1>
	    </xenotypeSet>
  </PawnKindDef>
 

 <PawnKindDef ParentName="DigifoxPawnBase" Name="Villager_DigifoxChild">
